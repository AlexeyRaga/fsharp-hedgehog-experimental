<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Christer van der Meeren, Nikos Baxevanis, Jacob Stanley</Authors>
    <Version>0.1.0</Version>
    <PackageLicenseUrl>https://github.com/cmeeren/fsharp-hedgehog-experimental/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/cmeeren/fsharp-hedgehog-experimental</PackageProjectUrl>
    <Company />
    <Description>Hedgehog with batteries included: Auto-generators, extra combinators, and more.</Description>
    <PackageIconUrl>https://raw.githubusercontent.com/cmeeren/fsharp-hedgehog-experimental/master/img/SQUARE_hedgehog_300x300.png</PackageIconUrl>
    <PackageTags>f# fsharp testing</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>$(TargetDir)\$(RootNameSpace).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Gen.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Hedgehog" Version="0.8.2" />
    <PackageReference Include="TypeShape" Version="5.1.0" />
  </ItemGroup>

</Project>
